{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {
    },
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "arrays": "always",
                    "objects": "always",
                    "functions": "never",
                    "imports": "always",
                    "exports": "always",
                    "typeLiterals": "always"
                },
                "singleline": "never",
                "esSpecCompliant": true
            }
        ]
    },
    "rulesDirectory": [
    ]
}